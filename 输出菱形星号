#include<stdio.h>
int main()
{
	int i = 0;
	int n = 0;
	int m = 0;
	for (i = 0; i <= 5; i++)
	{
		for (n = 0; n <= 5 - i; n++)
		{
			printf(" ");
		}
		for (n = 0; n <= i * 2 ; n++)
		{
			printf("*");
		}
		printf("\n");
	}
	for (i =0; i <= 5; i++)
	{
		for (n = 0; n <= i; n++)
		{
			printf(" ");
		}
		for (n = 0; n <= 10 - 2 * i; n++)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;
}
