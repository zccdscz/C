#include<stdio.h>
//输出什么
int main()
{
	char a = -1;
	//10000000000000000000000000000001原码
	//11111111111111111111111111111110反码（负数符号位不变其他位取反）
	//11111111111111111111111111111111补码（负数反码加1）
	//char只能存8bit补码：11111111
	//输出%d是整型，整型提升后有符号数高位补符号位：
	//11111111111111111111111111111111
	//最后输出原码为-1
	signed char b = -1;
	//和char a=-1一样
	unsigned char c = -1;
	//10000000000000000000000000000001原码
        //11111111111111111111111111111110反码（负数符号位不变其他位取反）
        //11111111111111111111111111111111补码（负数反码加1）
	//char存8bit补码：11111111
	//整型提升，无符号数高位补0：
	//0000000000000000000000001111111原码
	//正数原反补一样
	//输出原码11111111十进制为255
	printf("a=%d b=%d c=%d", a, b, c);
	return 0;
}
